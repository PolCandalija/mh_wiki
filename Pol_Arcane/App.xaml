<Application x:Class="mh.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:mh"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>

            <ResourceDictionary.MergedDictionaries>


                <ResourceDictionary Source="styles/Page.xaml" />
                <ResourceDictionary Source="styles/Image.xaml" />
                <ResourceDictionary Source="styles/Text.xaml" />
                <ResourceDictionary Source="styles/Button.xaml" />
                <ResourceDictionary Source="utilites/DataTemplate.xaml" />
                <ResourceDictionary Source="styles/MyFont.xaml" />
                <ResourceDictionary Source="styles/MyButton.xaml" />


            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>
    </Application.Resources>
</Application>
