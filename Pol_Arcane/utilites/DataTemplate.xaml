<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:mh.viewModel"
                    xmlns:view="clr-namespace:mh.view">
    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MonstersVM}">
        <view:Monsters />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:WeaponsVM}">
        <view:Weapons />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SkillsVM}">
        <view:Skills />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MapsVM}">
        <view:Maps />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ArmorsVM}">
        <view:Armors />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:WeaponsListVM}">
        <view:WeaponsList />
    </DataTemplate>

</ResourceDictionary>